{
	"name": "mariacdadalt/plugin-template",
	"description": "Maria C. Dadalt's Plugin Template",
	"type": "wordpress-plugin",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "https://mariacdadalt.github.io/composer-satis/"
		},
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"mariacdadalt/plugin-core": "dev-main"
	},
	"require-dev": {
		"inpsyde/php-coding-standards": "^1.0",
		"squizlabs/php_codesniffer": "^3.6",
		"brain/monkey": "^2",
		"phpunit/phpunit": "^9"
	},
	"autoload": {
		"psr-4": {
			"Plugin\\Template\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Plugin\\Template\\Tests\\Unit\\": "tests/PHPUnit/Unit/"
		}
	},
	"extra": {},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}